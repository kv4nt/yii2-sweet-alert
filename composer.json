{
    "name": "kv4nt/yii2-sweet-alert",
    "description": "A Yii2 widget for SweetAlert",
    "type": "yii2-extension",
    "keywords": ["yii2", "sweet alert", "yii2 alert", "sweetalert"," yi2i sweet alert"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Shumeyko",
            "email": "kvant90@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/bootstrap-sweetalert" : "*"
    },
    "autoload": {
        "psr-4": {
            "kv4nt\\sweetalert\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
